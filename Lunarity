       Key = {"Hello"} 
    }
})

local Tab = Window:CreateTab("‚öîÔ∏è - Main") 
local Tab2 = Window:CreateTab("üíé - Crystals") 
local Tab3 = Window:CreateTab("‚úàÔ∏è - Teleport")
local Tab4 = Window:CreateTab("üéÅ - GUIs")
local Tab5 = Window:CreateTab("üí° - Misc") 

local Section = Tab:CreateSection("Autofarm Section")
local Toggle = Tab:CreateToggle({
    Name = "Auto Swing",
    CurrentValue = false,
    Flag = "Toggle1", -- 
    Callback = function(Value)
        AutoSwing = Value
        if AutoSwing then
            wait(0.1)
            Swing()
        end
    end,
})  

local Toggle = Tab:CreateToggle({
    Name = "Auto Sell",
    CurrentValue = false,
    Flag = "Toggle2", -- 
    Callback = function(Value)
        AutoSell = Value
        while AutoSell do
            wait(0.1)
            Sell()
        end
    end,
})

local Toggle = Tab:CreateToggle({
    Name = "Auto Hoop",
    CurrentValue = false,
    Flag = "Toggle3", -- 
    Callback = function(Value)
        AutoHoop = Value
        while AutoHoop do
            wait(0.1)
            Hoop()
        end
    end,
})

local Button = Tab:CreateButton({
    Name = "Auto Unlock All Islands",
    Callback = function()
        UnlockIslands()
    end,
 })

 local Button = Tab:CreateButton({
    Name = "Auto Claim All Chests",
    Callback = function()
        ChestTp()
    end,
 })


local Section = Tab:CreateSection("Shop Section")

local Toggle = Tab:CreateToggle({
    Name = "Auto Buy Weapon",
    CurrentValue = false,
    Flag = "Toggle5", -- 
    Callback = function(Value)
        AutoBuyWeapon = Value
        while AutoBuyWeapon do
            wait(0.1)
            BuyWeapon()
            if not AutoBuyWeapon then break end
        end
    end,
})

local Toggle = Tab:CreateToggle({
    Name = "Auto Buy Belt",
    CurrentValue = false,
    Flag = "Toggle6", -- 
    Callback = function(Value)
        AutoBuyBelt = Value
        while AutoBuyBelt do
            wait(0.1)
            BuyBelt()
            if not AutoBuyBelt then break end
        end
    end,
})

local Toggle = Tab:CreateToggle({
    Name = "Auto Upgrade Rank",
    CurrentValue = false,
    Flag = "Toggle7", -- 
    Callback = function(Value)
        AutoBuyRank = Value
        while AutoBuyRank do
            wait(0.1)
            BuyRank()
            if not AutoBuyRank then break end
        end
    end,
})

local Section = Tab2:CreateSection("Crystal Section")
local CrystalValue

local Dropdown = Tab2:CreateDropdown({
    Name = "Choose A Crystal",
    Options = CrystalTable,
    CurrentOption = {"None"},
    MultipleOptions = false,
    Flag = "Dropdown1", 
    Callback = function(Option)
        CrystalValue = Option[1]
    end,
})

local Toggle = Tab2:CreateToggle({
    Name = "Open Crystal",
    CurrentValue = false,
    Flag = "Toggle",
    Callback = function(Value)
    AutoCrystal = Value
    while AutoCrystal do wait(0.5)
        local ohString1 = "openCrystal"
        local ohString2 = CrystalValue
        game:GetService("ReplicatedStorage").rEvents.openCrystalRemote:InvokeServer(ohString1, ohString2)
    end
end,
})

