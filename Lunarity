local AreaToTp
local Dropdown = Tab3:CreateDropdown({
    Name = "Choose An Island",
    Options = IslandTable,
    CurrentOption = {"None"},
    MultipleOptions = false,
    Flag = "Dropdown1", 
    Callback = function(Option)
        AreaToTp = Option[1]
    end,
 })

