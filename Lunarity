    Callback = function()
        Player.multiJumpCount.Value = 99999999
    end,
 })

function ChestTp()
    gamePath.mythicalChest["circleInner"].CFrame = Root.CFrame
    wait(1) 
    gamePath.goldenChest["circleInner"].CFrame = Root.CFrame
    wait(1) 
    gamePath.enchantedChest["circleInner"].CFrame = Root.CFrame
    wait(1) 
    gamePath.magmaChest["circleInner"].CFrame = Root.CFrame
    wait(1) 
    gamePath.legendsChest["circleInner"].CFrame = Root.CFrame 
    wait(1)
    gamePath.eternalChest["circleInner"].CFrame = Root.CFrame
    wait(1) 
    gamePath.saharaChest["circleInner"].CFrame = Root.CFrame
    wait(1) 
    gamePath.thunderChest["circleInner"].CFrame = Root.CFrame
    wait(1) 
    gamePath.ancientChest["circleInner"].CFrame = Root.CFrame
    wait(1) 
    gamePath.midnightShadowChest["circleInner"].CFrame = Root.CFrame
    wait(1) 
    gamePath["wonderChest"].circleInner.CFrame = Root.CFrame
    wait(1) 
    gamePath.ultraNinjitsuChest["circleInner"].CFrame = Root.CFrame 
end
